    "class Solution(object):\n",
    "    def mySqrt(self, x):\n",
    "        \"\"\"\n",
    "        :type x: int\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        if x == 0:\n",
    "            return 0\n",
    "        ans = float(x)\n",
    "        while True:\n",
    "            tmp = ans\n",
    "            ans = (ans + x / ans) / 2\n",
    "            if abs(ans - tmp) < 1:\n",
    "                break\n",
    "        return int(ans) \n",
    "        \n",
    "res = Solution().mySqrt(6)\n",
    "res"
   ]
