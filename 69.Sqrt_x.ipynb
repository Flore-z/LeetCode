    "# binary search
    "class Solution(object):\n",
    "    def mySqrt(self, x):\n",
    "        \"\"\"\n",
    "        :type x: int\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        i = 1\n",
    "        j = int(x/2) + 1\n",
    "        while i <= j:\n",
    "            y = int((i + j)/2)\n",
    "            if y ** 2 == x:\n",
    "                return y\n",
    "            elif y ** 2 > x:\n",
    "                j = y - 1\n",
    "            else:\n",
    "                i = y + 1\n",
    "        \n",
    "        return j\n",
    "\n",
    "res = Solution().mySqrt(6)\n",
    "res"
   ]
